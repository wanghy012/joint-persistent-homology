mask = zeros(100,40);
s = zeros(100,40);
for i=1:20
    for t = 1:100
        if rand()<0.1
            mask(t,i)=1;
            s(t,i) = f(t+)
        end
    end
end
for i=21:40
    sample = [];
    for t = 1:100
        if rand()<0.1
            sample(end+1,:) = [t,f2(t)+rand()*2];
        end
    end
    samples(i) = libpointer('doublePtrPtr',sample);
end